{
    "name": "bundle-backend",
    "version": "1.0.0",
    "description": "",
    "author": "Roman Pshenichnyy",
    "private": true,
    "license": "UNLICENSED",
    "engines": {
        "node": "^18.13.0",
        "npm": ">=8.19.3",
        "yarn": "yarn is not supported, please use npm"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/LaddeRHTML/bundle-back"
    },
    "scripts": {
        "deploy": "sls deploy",
        "prebuild": "sudo rimraf dist",
        "initialize": "node -e \"if(process.platform === 'win32') { require('child_process').execSync('.\\\\initialize.sh', { stdio: 'inherit' }) } else { require('child_process').execSync('chmod +x ./initialize.sh && ./initialize.sh', { stdio: 'inherit' }) }\"",
        "test": "editorconfig-checker",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": " set NODE_ENV=dev && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "start:local": "set NODE_ENV=local && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
        "up": "docker-compose up -d",
        "up:prod": "docker-compose -f docker-compose.prod.yml up -d",
        "down": "docker-compose down",
        "down:prod": "docker-compose -f docker-compose.prod.yml down",
        "prepare": "husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "./commands/pre-commit",
            "pre-push": "./commands/pre-push $HUSKY_GIT_STDIN"
        }
    },
    "dependencies": {
        "@nestjs/common": "^9.3.9",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.3.9",
        "@nestjs/jwt": "^10.0.2",
        "@nestjs/mapped-types": "*",
        "@nestjs/mongoose": "^9.2.1",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.3.9",
        "@nestjs/platform-fastify": "^9.3.9",
        "@nestjs/swagger": "^6.2.1",
        "@nestjs/typeorm": "^9.0.1",
        "@nestjs/cli": "^9.2.0",
        "@types/cache-manager": "^4.0.2",
        "bcryptjs": "^2.4.3",
        "cache-manager": "^4.1.0",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie-parser": "^1.4.6",
        "express": "^4.18.2",
        "husky": "^8.0.3",
        "moment": "^2.29.4",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.9.0",
        "rimraf": "^4.4.1",
        "rxjs": "^7.8.0",
        "swagger-ui-express": "^4.6.2",
        "typeorm": "^0.3.12"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.21.0",
        "@nestjs/schematics": "^9.0.4",
        "@nestjs/testing": "^9.3.9",
        "@types/aws-lambda": "^8.10.111",
        "@types/bcryptjs": "^2.4.2",
        "@types/cache-manager-redis-store": "^2.0.1",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.17",
        "@types/jest": "29.4.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.14.6",
        "@types/passport-jwt": "^3.0.8",
        "@types/passport-local": "^1.0.35",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "editorconfig-checker": "^5.0.1",
        "eslint": "^8.35.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.4.3",
        "prettier": "^2.8.4",
        "run-script-webpack-plugin": "0.1.1",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "~4.8.4",
        "webpack": "^5.75.0",
        "webpack-node-externals": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
